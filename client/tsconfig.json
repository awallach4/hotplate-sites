{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "preact/*": ["./types__react"]
    },
    "types": ["vuetify"]
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
